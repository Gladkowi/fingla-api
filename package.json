{
   "name": "fingla",
   "version": "0.2.1",
   "description": "",
   "author": "igor gladkov",
   "private": true,
   "license": "MIT",
   "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "start": "ts-node -r tsconfig-paths/register src/main.ts",
      "start:dev": "cross-env NODE_PATH=./src/ nodemon",
      "start:debug": "nodemon --config nodemon-debug.json",
      "prestart:prod": "rimraf dist && npm run build",
      "start:prod": "cross-env NODE_ENV=production NODE_PATH=dist/ node dist/main.js",
      "lint": "tslint -p tsconfig.json -c tslint.json",
      "lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
      "sync:diff": "ts-node ./node_modules/.bin/typeorm schema:log > schemalog.sql",
      "seed:user": "ts-node ./src/user/user.seed.ts",
      "seed:queue:tasks": "ts-node src/serviceworker.ts",
      "seed:queue:tasksReady": "ts-node ./src/procVideo/procVideo.ready.seed.ts",
      "seed:uploadTasks": "ts-node ./src/services/s3/s3.seed.ts",
      "queue:dev": "ts-node -r tsconfig-paths/register src/queue.ts",
      "queue": "cross-env NODE_ENV=production NODE_PATH=dist/ node dist/queue.js",
      "typeorm:config": "ts-node -r tsconfig-paths/register src/main.ts --typeorm-export-only",
      "typeorm:sync": "npm run typeorm schema:sync",
      "typeorm": "npm run typeorm:config && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "initialsizes:dev": "ts-node -r tsconfig-paths/register src/scripts/initializeFileSizes.script.ts",
      "process:dev": "ts-node -r tsconfig-paths/register src/scripts/checkSize.script.ts"
   },
   "dependencies": {
      "@hapi/joi": "^17.1.1",
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^1.1.5",
      "@nestjs/core": "^8.0.0",
      "@nestjs/jwt": "^8.0.0",
      "@nestjs/passport": "^8.1.0",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/platform-socket.io": "^8.4.4",
      "@nestjs/swagger": "^5.2.0",
      "@nestjs/typeorm": "^8.0.3",
      "@nestjs/websockets": "^8.4.4",
      "@types/geojson": "^7946.0.8",
      "@types/hapi__joi": "^17.1.8",
      "argon2": "^0.28.5",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "convict": "^4.4.1",
      "cookie-parser": "^1.4.6",
      "cross-env": "^5.2.0",
      "date-fns": "^2.28.0",
      "dotenv": "^10.0.0",
      "fastify-swagger": "^5.0.0",
      "mailgun.js": "^2.0.1",
      "moment": "^2.29.1",
      "mysql": "^2.14.1",
      "passport": "^0.5.2",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "pg": "^8.7.1",
      "rand-token": "^1.0.1",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.5.2",
      "swagger-ui-express": "^4.3.0",
      "typeorm": "0.2.41"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/convict": "^4.2.1",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.13",
      "@types/jest": "27.0.2",
      "@types/node": "^8.0.29",
      "@types/passport": "^1.0.7",
      "@types/passport-jwt": "^3.0.6",
      "@types/passport-local": "^1.0.34",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "jest": "^27.2.5",
      "nodemon": "^2.0.15",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "toml": "^3.0.0",
      "ts-jest": "^27.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.10.1",
      "tslint": "5.15.0",
      "typescript": "^3.8.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
